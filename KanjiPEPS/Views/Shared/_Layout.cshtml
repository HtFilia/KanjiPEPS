<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - KanjiWebApp</title>
    <!-- Font Awesome Icons -->
    <link rel="stylesheet" href="~/css/all.css" />
    <!-- Bootstrap -->
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <!-- Own's CSS -->
    <link rel="stylesheet" href="~/css/site.css" />
    <!-- Argon Theme -->
    <link rel="stylesheet" href="~/css/argon.css" />
</head>
<body>
    <nav id="sidenav-main" class="navbar navbar-vertical fixed-left navbar-expand-md navbar-light bg-white">
        <div class="container-fluid">
            <a class="navbar-brand" asp-controller="Home">
                <img src="~/logo.png" />
            </a>
            <div id="sidenav-collapse-main" class="collapse navbar-collapse">
                <hr class="my-3" />
                <ul id="navbar-nav" class="navbar-nav">
                    <li class="nav-item active">
                        <a class="nav-link" asp-controller="Home">
                            <i class="fas fa-coins"></i>
                            <span>Options</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#">
                            <i class="fas fa-chart-line"></i>
                            <span>Évolution des sous-jacents</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#">
                            <i class="fas fa-home"></i>
                            <span>Couverture</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#">
                            <i class="fas fa-vial"></i>
                            <span>Backtesting</span>
                        </a>
                    </li>
                </ul>
                <hr class="my-3" />
            </div>
        </div>
    </nav>
    <div class="main-content">
        <!-- Page Content -->
        <div class="container-fluid">
            @RenderBody()
        </div>
    </div>
    <!--
    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2020 - KanjiWebApp - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
        -->
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <!-- Chart.js -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.9.3/Chart.bundle.js" integrity="sha256-8zyeSXm+yTvzUN1VgAOinFgaVFEFTyYzWShOy9w7WoQ=" crossorigin="anonymous"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
